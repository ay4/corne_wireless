        behaviors {
        // 
        // CMD_SP AND ALT_SP ARE DEPRECATED
        // AND AREN'T USED IN THIS KEYMAP    
        // LEAVING JUST FYI    
        //    
        // Tap-Hold: Macro if held,
        // keycode if tapped. The macros
        // in question press Command or Option
        // and switch layer to QWERTY. I need this
        // because I want all shortcuts to be in 
        // the QWERTY layout so that my muscle memory
        // isn't lost. 
        //    
        cmd_sp: cmd_sp {
            compatible = "zmk,behavior-hold-tap";
            label="CMD_SP";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            quick-tap-ms = <125>;
            bindings = <&cmd_qwerty>, <&kp>;
        };
  
        alt_sp: alt_sp {
            compatible = "zmk,behavior-hold-tap";
            label="ALT_SP";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            quick-tap-ms = <125>;
            bindings = <&alt_qwerty>, <&kp>;
        };

        //
        // Custom Layer-Tap: switches to layer when held,
        // presses a button when pressed. ZMK has this
        // by default (called &lt), but I duplicate it
        // because I want to play with tapping terms and such.
        //
        lt_cust: lt_cust {
            compatible = "zmk,behavior-hold-tap";
            label="LT_CUSTOM";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <250>;
            quick-tap-ms = <125>;
            bindings = <&mo>, <&kp>;
        };

        //
        // Same as lt_cust, but calls macros when held.
        // I use this to switch between languages. I wrote
        // language-switching macros that (a) switch my active
        // layer; (b) switch the OS language.
        //
        hold_ru: hold_ru {
            compatible = "zmk,behavior-hold-tap";
            label="hold_ru";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <250>;
            quick-tap-ms = <125>;
            bindings = <&to_ru>, <&kp>;
        };

        hold_en: hold_en {
            compatible = "zmk,behavior-hold-tap";
            label="hold_en";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <250>;
            quick-tap-ms = <125>;
            bindings = <&to_en>, <&kp>;
        };

        //
        // These two are used to go to English symbol layers
        // when being held in the Russian layout and produce
        // keypresses if they're tapped, not held.
        //
        hold_sym_l: hold_sym_l {
            compatible = "zmk,behavior-hold-tap";
            label="hold_sym_l";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <250>;
            quick-tap-ms = <125>;
            bindings = <&en_sym_l>, <&kp>;
        };
        hold_sym_r: hold_sym_r {
            compatible = "zmk,behavior-hold-tap";
            label="hold_sym_r";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <250>;
            quick-tap-ms = <125>;
            bindings = <&en_sym_r>, <&kp>;
        };

    };



